<h1>Add a new instructor: </h1>


<%= form_with model: @instructor, url: instructors_path do |form| %>
  <h2> First name: </h2> <%= form.text_field :first_name %>
  <h2> Last name: </h2> <%= form.text_field :last_name %>
  <h2> Age: </h2> <%= form.text_field :age %>
  <h2> Salary: </h2> <%= form.text_field :salary %>
  <h2> Highest completed education: </h2> <%= form.text_field :highest_comp_ed %>
  </br>
   </br>
    </br>
  <%= form.submit %>
<% end %>


